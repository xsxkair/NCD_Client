<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXDatePicker?>
<?import com.jfoenix.controls.JFXSpinner?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Pagination?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="765.0" prefWidth="1247.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <StackPane prefHeight="150.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <AnchorPane prefHeight="200.0" prefWidth="200.0">
               <children>
                  <HBox alignment="CENTER_LEFT" spacing="30.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="10.0">
                     <children>
                        <HBox alignment="CENTER" spacing="10.0">
                           <children>
                              <Label text="起始日期：" />
                              <JFXDatePicker fx:id="GB_StartDateField" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER" spacing="10.0">
                           <children>
                              <Label text="结束日期：" />
                              <JFXDatePicker fx:id="GB_EndDateField" />
                           </children>
                        </HBox>
                        <JFXButton onAction="#GB_QueryRecordAction" prefHeight="34.0" prefWidth="106.0" ripplerFill="#da0794" style="-fx-background-color: mediumaquamarine;" text="查    询">
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                        </JFXButton>
                     </children>
                  </HBox>
                  <TableView fx:id="GB_CardTableView" prefHeight="650.0" AnchorPane.bottomAnchor="55.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="60.0">
                     <columns>
                        <TableColumn fx:id="GB_TableColumn1" editable="false" prefWidth="75.0" text="时间" />
                        <TableColumn fx:id="GB_TableColumn2" editable="false" prefWidth="75.0" text="试剂卡批号" />
                        <TableColumn fx:id="GB_TableColumn3" editable="false" prefWidth="75.0" text="试剂卡项目" />
                        <TableColumn fx:id="GB_TableColumn4" editable="false" prefWidth="75.0" text="数目" />
                        <TableColumn fx:id="GB_TableColumn5" editable="false" prefWidth="75.0" text="操作人" />
                        <TableColumn fx:id="GB_TableColumn6" editable="false" prefWidth="75.0" text="领料人" />
                        <TableColumn fx:id="GB_TableColumn7" editable="false" prefWidth="75.0" text="领料设备" />
                     </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
                  <Pagination fx:id="GB_Pagination" maxPageIndicatorCount="20" styleClass="bullet" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
               </children>
            </AnchorPane>
            <VBox fx:id="GB_FreshPane" alignment="CENTER" spacing="10.0" style="-fx-background-color: rgba(0, 0, 0, 0.2);" visible="false">
               <children>
                  <JFXSpinner startingAngle="0.0" style="-fx-radius: 50;" />
                  <Label text="正在加载数据，请稍后" />
               </children>
               <cursor>
                  <Cursor fx:constant="WAIT" />
               </cursor>
            </VBox>
         </children>
      </StackPane>
   </children>
</AnchorPane>
